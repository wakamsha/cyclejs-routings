  "dependencies": {
    "@cycle/dom": "^20.1.0",
    "@cycle/history": "^6.10.0",
    "@cycle/run": "^4.1.0",
    "@cycle/rxjs-run": "^8.0.0",
    "@types/ramda": "^0.25.17",
    "cyclic-router": "^5.1.7",
    "ramda": "^0.25.0",
